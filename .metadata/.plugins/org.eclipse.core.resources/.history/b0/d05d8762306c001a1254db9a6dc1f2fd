<script type="text/javascript">
	function(sum){
		var i = 0;
		var _sum = 0;
		for(i = 0; i < arguments.length; i++){
			document.write(i + " : " + arguments[i] + "<br>")
			_sum = _sum + arguments[i]
		}
		return _sum
	}
	document.write("result : " + sum(1,2,3,4))
</script>
