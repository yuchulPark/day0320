	var person = {name:"홍길동", age:20}; // 자바스크립트의 객체. 객체를 만들음
	
	str = "age의 자료형 : " + typeof(age) + "\n";
	str += "name의 자료형 : " + typeof(name) + "\n";
	str += "height의 자료형 : " + typeof(height) + "\n";
	str += "arr의 자료형 : " + typeof(arr) + "\n";
	str += "person의 자료형 : " + typeof(person) + "\n";
	alert(str);
</script>
